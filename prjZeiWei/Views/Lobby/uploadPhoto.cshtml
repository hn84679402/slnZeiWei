<style>
    body{
        background-color:black;
    }
</style>
@model prjZeiWei.Models.MemberInfo

@{
    ViewBag.Title = "uploadPhoto";
}

<h2 style="color:white">上傳您的大頭貼</h2>

@using (Html.BeginForm())
{
}
<form action="@Url.Action("uploadPhoto")" method="post" enctype="multipart/form-data">
    <div class="form-horizontal">
        <img id="photoShow" />
        <div>
            <div class="col-md-10">
                <input id="File1" name="image" type="file" class="form-control" accept="image/gif, image/jpeg, image/png/">
            </div>
        </div>
        <div class="form-group">
            <div class="col-md-10">
                <input type="submit" value="上傳" class="btn btn-default" />
                <h5>@ViewBag.Message</h5>
            </div>
        </div>
    </div>
</form>


<div>
    @Html.ActionLink("回首頁", "Index")
</div>
